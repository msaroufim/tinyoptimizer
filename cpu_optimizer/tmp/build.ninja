ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=cpu_adam -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /opt/conda/envs/offload/lib/python3.10/site-packages/torch/include -isystem /opt/conda/envs/offload/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -isystem /opt/conda/envs/offload/lib/python3.10/site-packages/torch/include/TH -isystem /opt/conda/envs/offload/lib/python3.10/site-packages/torch/include/THC -isystem /opt/conda/envs/offload/include/python3.10 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++17 -I/home/ubuntu/tinyoptimizer/cpu_optimizer -std=c++2a
post_cflags = 
cuda_dlink_post_cflags = 
ldflags = -shared -L/opt/conda/envs/offload/lib/python3.10/site-packages/torch/lib -lc10 -ltorch_cpu -ltorch -ltorch_python

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc



rule link
  command = $cxx $in $ldflags -o $out

build cpu_adam_impl.o: compile /home/ubuntu/tinyoptimizer/cpu_optimizer/cpu_adam_impl.cpp



build cpu_adam.so: link cpu_adam_impl.o

default cpu_adam.so
